<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import presentation.panel.HotkeyPanel?>
<HotkeyPanel fx:id="hotkeyContainer" alignment="CENTER" prefHeight="400.0" prefWidth="600.0"
             xmlns="http://javafx.com/javafx/11.0.1"
             xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.HotkeyScene">
    <Button fx:id="backButton" alignment="CENTER" contentDisplay="CENTER" text="Back"/>
    <ListView fx:id="hotkeyList" prefHeight="200.0" prefWidth="200.0"/>
    <HBox prefHeight="100.0" prefWidth="100.0">
        <children>
            <Spinner fx:id="mobileSpinner"/>
            <Spinner fx:id="desktopSpinner"/>
        </children>
    </HBox>

    <Button fx:id="addButton" alignment="CENTER" contentDisplay="CENTER" text="Add"/>

</HotkeyPanel>
